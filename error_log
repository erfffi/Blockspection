      SELECT tr.id,
             IF( tr.subscription_id = 0, 'none',
                 ( SELECT sub.status
                     FROM wp_mepr_subscriptions AS sub
                    WHERE sub.id = tr.subscription_id
                    LIMIT 1
                 )
             ) AS sub_status
        FROM wp_mepr_transactions AS tr
       WHERE tr.expires_at <> '0000-00-00 00:00:00'
         AND tr.status IN ( 'confirmed', 'complete' )
         AND DATE_ADD( tr.expires_at, INTERVAL 12 HOUR ) <= '2018-03-01 08:05:44'
         AND DATE_ADD( DATE_ADD( tr.expires_at, INTERVAL 12 HOUR ), INTERVAL 2 DAY ) >= '2018-03-01 08:05:44'
         AND ( SELECT ev.id
                 FROM wp_mepr_events AS ev
                WHERE ev.evt_id=tr.id
                  AND ev.evt_id_type='transactions'
                  AND (ev.event='expired' OR ev.event='transaction-expired')
                LIMIT 1 ) IS NULL
       ORDER BY tr.expires_at
